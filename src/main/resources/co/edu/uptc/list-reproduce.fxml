<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@css/style.css" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uptc.view.users.ListReproduceView">
   <HBox spacing="20" AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0"> 
   <VBox id="vbox" spacing="10" HBox.hgrow="ALWAYS" alignment="CENTER">
        <TableView fx:id="tableView" styleClass="table-view">
            <columns>
                <TableColumn text="id" fx:id="idColumn" />
                <TableColumn text="Name " fx:id="nameColumn" />
                <TableColumn text="Synopsisp" fx:id="synopsispColumn" />
                <TableColumn text="Duration" fx:id="durationColumn" />
                <TableColumn text="Date" fx:id="dateColumn" />
            </columns>
        </TableView>
        </VBox>
         <Separator styleClass="line" orientation="VERTICAL"/>

        <VBox id="vbox" spacing="10" HBox.hgrow="ALWAYS" alignment="CENTER">
        <Button fx:id="createButton" text="add" onAction="#saveMovie"/>
        <Button fx:id="backButton" text="delete" />
        <Button fx:id="backButton" text="update" />
        <Button fx:id="backButton" text="find" />
        <Button fx:id="backButton" text="obtain all" />
         </VBox>
    </HBox>
</AnchorPane>
