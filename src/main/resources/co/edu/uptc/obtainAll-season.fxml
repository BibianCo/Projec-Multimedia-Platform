<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.ComboBox?>


<StackPane id="pane" prefHeight="700" prefWidth="700" stylesheets="@css/style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uptc.view.seasons.ObtainAllSeasonView">
    <children>
        <HBox spacing="5">

             <VBox id="vbox" spacing="10" HBox.hgrow="ALWAYS" alignment="CENTER">


                <Label id="title" alignment="CENTER" text="Existing Season"/>
                <children>
                    <TableView fx:id="tableView"  styleClass="table-view">
                        <columns>
                          <TableColumn text="id" fx:id="idColumn" />
                            <TableColumn text="number" fx:id="numberColumn" />
                            <TableColumn text="idSerie" fx:id="idSerieColumn" />
                            <TableColumn text="episodes" fx:id="episodesColumn" />
                        </columns>
                    </TableView>
                     <Button fx:id="backButton" text="Back" onAction="#sceneMenu"/>

                    
                </children>
                
                
            </VBox>
           
        </HBox>
    </children>
</StackPane>